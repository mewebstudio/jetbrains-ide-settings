<keymap version="1" name="Default copy" parent="Mac OS X 10.5+">
  <action id="CollapseAllRegions">
    <keyboard-shortcut first-keystroke="shift meta minus" />
  </action>
  <action id="ExpandAllRegions">
    <keyboard-shortcut first-keystroke="shift meta 0" />
  </action>
  <action id="ExpandRegion">
    <keyboard-shortcut first-keystroke="meta 0" />
  </action>
  <action id="android.deploy.ApplyChanges">
    <keyboard-shortcut first-keystroke="ctrl meta e" />
  </action>
  <action id="android.deploy.CodeSwap">
    <keyboard-shortcut first-keystroke="shift ctrl meta e" />
  </action>
</keymap>